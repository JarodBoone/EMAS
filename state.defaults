#!/bin/bash
# This shell script defines state variables for the EMAS interface. Variables 
# are automatically managed by the interface scripts 
#
# =========================================
# DO NOT CHANGE THESE VARIABLES MANUALLY
# =========================================

# has EMAS been configured? 
export __EMAS_CONFIG=0

# do we have the right directory structure?
export __EMAS_DIR_CHECK=0

# has any gem5 binary been built at all? 
export __BUILD_DONE=0

# how many gem5 binaries have been built 
export __NUM_BUILT=0

# the name of the previous scenario run
export __PREV_SCENARIO=""